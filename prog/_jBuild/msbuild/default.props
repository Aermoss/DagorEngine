    <NMakeIncludeSearchPath>$(NMakeIncludeSearchPath);$(CommonProgDir)engine2\sharedInclude;$(CommonProgDir)tools\sharedInclude;$(CommonProgDir)gameLibs\publicInclude;$(CommonProgDir)gameLibs\publicInclude\quirrel;$(CommonProgDir)gameLibs\ecs\input;$(CommonProgDir)commonFx\commonFxGame;$(_1stPartyLibs);$(_3rdPartyLibs);$(CommonProgDir)engine2;$(SolutionDir)launcher\client\updater\include</NMakeIncludeSearchPath>
